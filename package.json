{
  "name": "chestnut",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clean": "rm -rf build/*",
    "svg": "svgr -d src/js/icons src/icons",
    "lint": "eslint src/js/*",
    "lint:fix": "eslint --fix src/js/*",
    "lint-css": "stylelint 'src/sass/**/*.scss'",
    "lint-css:fix": "npm run lint-css -- --fix",
    "precommit": "npm run lint-css:fix && npm run lint:fix && npm run prod"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@svgr/cli": "^5.5.0",
    "@wordpress/base-styles": "^3.3.3",
    "@wordpress/block-editor": "^5.2.10",
    "@wordpress/blocks": "^7.0.6",
    "@wordpress/components": "^12.0.8",
    "@wordpress/compose": "^3.24.5",
    "@wordpress/data": "^4.26.8",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.0.1",
    "@wordpress/element": "^2.19.1",
    "@wordpress/eslint-plugin": "^8.0.2",
    "@wordpress/hooks": "^2.11.1",
    "@wordpress/i18n": "^3.18.0",
    "@wordpress/icons": "^2.9.1",
    "@wordpress/server-side-render": "^1.20.8",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.3",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.22.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsdoc": "^30.7.13",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass": "^1.32.8",
    "sass-loader": "^9.0.3",
    "sass-mq": "^5.0.1",
    "style-loader": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "bootstrap": "^4.6.0"
  }
}
